{
  "name": "startup-gulp-template",
  "version": "1.2.0",
  "description": "Lightweight startup Gulp with SASS template for web project",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp",
    "build": "cross-env NODE_ENV=production gulp build",
    "deploy": "npm run build && gh-pages -d build",
    "dist": "npm run build && gulp zip",
    "lintspaces": "gulp lintspaces",
    "stylelint": "stylelint \"source/sass/**/*.{scss,sass}\" --syntax scss",
    "eslint": "eslint \"source/js/**/*.js\"",
    "test": "npm run lintspaces && npm run stylelint && npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/embyth/startup-gulp-template.git"
  },
  "author": "Rostyslav Miniukov <miniukovrostyslav@gmail.com> (https://github.com/embyth)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/embyth/startup-gulp-template/issues"
  },
  "homepage": "https://github.com/embyth/startup-gulp-template#readme",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "autoprefixer": "^9.8.6",
    "browser-sync": "^2.26.12",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "eslint": "^7.6.0",
    "gh-pages": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-changed": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^4.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-lintspaces": "^1.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-svgstore": "^7.0.1",
    "gulp-ttf2woff": "^1.1.1",
    "gulp-ttf2woff2": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-webp": "^4.0.1",
    "gulp-zip": "^5.0.2",
    "husky": "^4.2.5",
    "imagemin-jpegoptim": "^7.0.0",
    "require-dir": "^1.2.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "normalize.css": "^8.0.1",
    "picturefill": "^3.0.3",
    "svg4everybody": "^2.1.9"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  }
}
